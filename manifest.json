{
  "manifest_version": 3,
  "name": "React Content Script",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "permissions": [
    "cookies",
    "storage",
    "http://*.com/",
    "https://*.com/"
  ],
  "host_permissions": [
    "http://*.com/*",
    "https://*.com/*"
  ],
  "background": {
    "service_worker": "content-script/src/service/background.ts"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script/src/main.tsx"],
      "media": []
    }
  ]
}
